.page-header
  %h1 批量创建晓书用户
    
= form_tag '/users/batch_create', :multipart => true, :html => {:class => "form-horizontal"} do
  %fieldset
    .control-group
      = label_tag :csv, ".csv文件", :class => "control-label"
      .controls
        = file_field_tag :csv, :class => "input-xlarge"
    .form-actions
      = submit_tag "上传", :class => "btn btn-primary"
